<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lyny.mapper.GPS_lbsDataMapper">
    <!--插入lbs信息-->
    <insert id="insert" parameterType="GPS_lbsData">
        insert into gps_lbsdata(id,mcc,mnc,lac,cellid,mciss,
        deviceId,time)
        values(#{id},#{mcc},#{mnc},#{lac},#{cellid},
        #{mciss},#{deviceId},#{time})
    </insert>
</mapper>