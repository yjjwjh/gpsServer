<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lyny.mapper.GPS_wifiDataMapper">
    <!--插入wifi信息-->
    <insert id="insert" parameterType="GPS_lbsData">
        insert into gps_wifidata(id,bssid,rssi,time,deviceId)
        values(#{id},#{bssid},#{rssi},#{time},#{deviceId})
    </insert>
</mapper>